{
  "name": "digital-market-space-2.0-backend",
  "version": "2.0.0",
  "description": "Backend robuste pour marketplace digital - Missions freelance & Produits digitaux",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "prod": "NODE_ENV=production node src/server.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "clean": "rm -rf node_modules dist logs uploads",
    "setup": "npm install && npm run lint:fix && npm run format",
    "docker:build": "docker build -t digital-market-space-backend .",
    "docker:run": "docker run -p 3000:3000 digital-market-space-backend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.4.1",
    "cors": "^2.8.5",
    "jsonwebtoken": "^9.0.2",
    "@supabase/supabase-js": "^2.39.5",
    "bcryptjs": "^2.4.3",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-async-errors": "^3.1.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "uuid": "^9.0.1",
    "fedapay": "^1.0.0",
    "openai": "^4.52.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "eslint": "^9.10.0",
    "prettier": "^3.3.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "marketplace",
    "freelance",
    "digital-products",
    "express",
    "supabase",
    "fedapay",
    "nodejs",
    "backend",
    "api"
  ],
  "author": "Digital Market Space Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zouk5574-stack/Digital-Market-Space-2.0-B.git"
  }
}
